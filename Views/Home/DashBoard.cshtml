@model List<TTG3.Models.MyViewModel>
@{
    ViewBag.Title = "Dash Board";
}
<style>
    .red{
        color:red;
    }
    .green{
        color:green;
    }
</style>
<h2 class="text-center">Dash Board</h2>
<hr style="background-color:black"/>
<div class="row">
    <div class="col-4">
        <div class="card shadow" style="width:250px;height:200px;margin:auto">
            <div class="card-body" style="background-color: #31D67B">
                @if (ViewData["labCount"] != null)
                {
                    <h1 class="text-center font-weight-bold">@ViewData["labCount"]</h1>
                }
                else
                {
                    <h1 class="text-center font-weight-bold">0</h1>
                }
            </div>
            <div class="card-footer text-center">
                <p class="card-text">Lab Available</p>
                <button type="button" class="btn btn-block" style="background-color: #157DFA;color: black;font-weight: bold;" data-toggle="modal" data-target="#labListModal">
                    Lab List
                </button>
            </div>
        </div>
    </div>
    <div class="col-4">
        <div class="card shadow" style="width:250px;height:200px;margin:auto">
            <div class="card-body" style="background-color:#31D67B;">
                @if (ViewData["classroomCount"] != null)
                {
                    <h1 class="text-center font-weight-bold">@ViewData["classroomCount"]</h1>
                }
                else
                {
                    <h1 class="text-center font-weight-bold">0</h1>
                }
            </div>
            <div class="card-footer text-center">
                <p class="card-text">Room Available</p>
                <button type="button" class="btn btn-block" style="background-color: #157DFA;color: black;font-weight: bold;" data-toggle="modal" data-target="#roomListModal">
                    Room List
                </button>
            </div>
        </div>
    </div>
    <div class="col-4">
        <div class="card shadow" style="width:250px;height:200px;margin:auto">
            <div class="card-body" style="background-color:#31D67B;">
                @if (ViewData["theorySubjectCount"] != null)
                {
                    <h1 class="text-center font-weight-bold">@ViewData["theorySubjectCount"]</h1>
                }
                else
                {
                    <h1 class="text-center font-weight-bold">0</h1>
                }
            </div>
            <div class="card-footer text-center">
                <p class="card-text">Theory Subject</p>
                <button type="button" class="btn btn-block" style="background-color: #157DFA;color: black;font-weight: bold;" data-toggle="modal" data-target="#theorySubjectModal">
                    Theory Subject List
