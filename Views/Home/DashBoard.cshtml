<div class="modal fade" id="labListModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Labs Available</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body text-center">
                <table class="table" style="font-size:12px">
                    @if (ViewBag.labList != null)
                    {
                        foreach (var item in ViewBag.labList)
                        {
                            <tr><td>@item.LabName</td></tr>
                        }
                    }
                </table>
            </div>
        </div>
    </div>
</div>
